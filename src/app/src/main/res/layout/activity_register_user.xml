<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:autofillHints="@string/activity_register_et_password_confirmation"
    tools:context=".RegisterUserActivity">

    <TextView
        android:id="@+id/title_Activity_Register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="211dp"
        android:layout_marginBottom="665dp"
        android:text="@string/title_Activity_Register"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="344dp"
        android:layout_height="521dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="132dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/activity_register_et_password_confirmation"
            android:layout_width="321dp"
            android:layout_height="52dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="464dp"
            android:layout_marginBottom="9dp"
            android:autofillHints="@string/activity_register_et_password_confirmation"
            android:ems="10"
            android:hint="@string/activity_register_et_password"
            android:inputType="textPassword"
            android:password="true"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_password_confirmation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="438dp"
            android:layout_marginBottom="61dp"
            android:labelFor="@id/activity_register_et_password_confirmation"
            android:text="@string/activity_register_tv_password_confirmation"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_lastname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="481dp"
            android:labelFor="@id/activity_register_et_firstname"
            android:text="@string/activity_register_tv_lastname"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/activity_register_et_firstname"
            android:layout_width="321dp"
            android:layout_height="52dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="122dp"
            android:layout_marginBottom="352dp"
            android:hint="@string/activity_register_et_firstname"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_firstname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="99dp"
            android:layout_marginBottom="398dp"
            android:labelFor="@id/activity_register_et_lastname"
            android:text="@string/activity_register_tv_firstname"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/activity_register_et_lastname"
            android:layout_width="321dp"
            android:layout_height="52dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="48dp"
            android:layout_marginBottom="434dp"
            android:hint="@string/activity_register_et_lastname"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/activity_register_et_mail"
            android:layout_width="321dp"
            android:layout_height="52dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="208dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="266dp"
            android:hint="@string/activity_register_et_mail"
            android:labelFor="@id/activity_register_tv_mail"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_mail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="185dp"
            android:layout_marginBottom="312dp"
            android:labelFor="@id/activity_register_et_mail"
            android:text="@string/activity_register_tv_mail"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="271dp"
            android:layout_marginBottom="226dp"
            android:labelFor="@id/activity_register_et_phone"
            android:text="@string/activity_register_tv_phone"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/activity_register_et_phone"
            android:layout_width="312dp"
            android:layout_height="52dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="302dp"
            android:layout_marginBottom="174dp"
            android:ems="10"
            android:hint="@string/activity_register_et_phone"
            android:inputType="number"
            android:labelFor="@id/activity_register_tv_phone"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/activity_register_tv_password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="362dp"
            android:layout_marginBottom="137dp"
            android:labelFor="@id/activity_register_et_password"
            android:text="@string/activity_register_tv_password"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/activity_register_et_password"
            android:layout_width="321dp"
            android:layout_height="52dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="388dp"
            android:layout_marginBottom="92dp"
            android:autofillHints="@string/activity_register_et_password"
            android:ems="10"
            android:hint="@string/activity_register_et_password"
            android:inputType="textPassword"
            android:password="true"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/activity_register_btn_create"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="658dp"
        android:layout_marginBottom="20dp"
        android:text="@string/activity_register_btn_create"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/activity_register_chk_terms_and_policy"
        android:layout_width="342dp"
        android:layout_height="52dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="606dp"
        android:layout_marginBottom="84dp"
        android:text="@string/activity_register_chk_terms_and_policy"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>